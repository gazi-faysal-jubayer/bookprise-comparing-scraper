{
  "build": {
    "src": "app.py@",
    "use": "@vercelpython"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    },
    {
          "src": "/static/(.*)",
          "dest": "../static/$1"
      },
      {
          "src": "/templates/(.*)",
          "dest": "../templates/$1"
      }
  ]
}